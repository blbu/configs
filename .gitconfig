[user]
    email = blake.j.burgess@gmail.com
    name = Blake Burgess
[credential]
    helper = cache --time=3600
[alias]
    co = checkout
    ci = commit
    st = status --untracked-files=no
    stu = status
    br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=local
    histrel = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=relative
    histreltoday = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=relative --since "yesterday"
    pullom = pull origin master
    pushom = push origin master
